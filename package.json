{
  "name": "n8n-nodes-axiomai",
  "version": "0.1.2",
  "description": "n8n community node to post data to axiom.ai",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "axiom.ai",
    "axiomai",
    "browser automation",
    "web scraping",
    "scraping",
    "data extraction",
    "automation",
    "rpa",
    "no-code",
    "form filling",
    "data entry"
  ],
  "license": "MIT",
  "homepage": "https://axiom.ai",
  "author": {
    "name": "Alex Barlow - Axiom.ai",
    "email": "support@axiom.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/thegreatwebco/axiom-n8n.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials --ext .ts",
    "lintfix": "eslint nodes credentials --ext .ts --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AxiomAiApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AxiomAi/AxiomAi.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "~5.2.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "n8n-core": "^1.0.0"
  }
}

